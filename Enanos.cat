    <profile id="d22e-454f-b6f0-0e1f" name="Cañón organo ligero" hidden="false" typeId="576561706f6e23232344415441232323" typeName="Arma">
      <characteristics>
        <characteristic name="Rango" typeId="52616e676523232344415441232323">12&quot;</characteristic>
