          </constraints>
          <profiles>
            <profile id="40f3-caa7-a73a-113c" name="Retazo de la capa de Van Horstmann" hidden="false" typeId="5d17-3511-0c13-e904" typeName="Objeto hechizado">
              <characteristics>
                <characteristic name="Reglas especiales" typeId="0ef8-5055-c22e-455a">Los enemigos en contacto peana con peana con el portador deberán repetir sus tiradas de salvación especial exitosas.</characteristic>
              </characteristics>
            </profile>
          </profiles>
          <costs>
            <cost name="pts" typeId="points" value="20.0"/>
          </costs>
        </selectionEntry>
        <selectionEntry id="b7de-591d-32d9-296d" name="Anillo de Volans" hidden="false" collective="false" import="true" type="upgrade">
