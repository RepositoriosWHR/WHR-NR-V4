            </selectionEntry>
            <selectionEntry id="8802-3f65-b2be-161f" name="Manto de Hashut" hidden="false" collective="false" import="true" type="upgrade">
              <constraints>
                <constraint field="selections" scope="parent" value="1.0" percentValue="false" shared="false" includeChildSelections="false" includeChildForces="false" id="6c34-0457-4564-bb3a" type="max"/>
