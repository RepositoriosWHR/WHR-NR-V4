    <profile id="4528-5a37-2fb3-4d58" name="Bombas de mano de Malakai Makaisson" hidden="false" typeId="576561706f6e23232344415441232323" typeName="Arma">
      <characteristics>
        <characteristic name="Rango" typeId="52616e676523232344415441232323">10&quot;</characteristic>
