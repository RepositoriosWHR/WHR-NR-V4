        <profile id="0559-3876-c3e6-c129" name="[4] - Congelación letal" hidden="false" typeId="aa67-99ab-2618-c451" typeName="Hechizo">
          <characteristics>
            <characteristic name="Tipo" typeId="3b39-64e9-beb6-7abc">Daño directo</characteristic>
            <characteristic name="Dificultad" typeId="e3cf-4d57-0b28-a094">12+/15+</characteristic>
