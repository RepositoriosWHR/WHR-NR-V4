    <selectionEntry id="5d96-34dd-dafe-e2e7" name="Carruaje de las sombras" hidden="false" collective="false" import="true" type="upgrade">
      <constraints>
        <constraint field="selections" scope="roster" value="1.0" percentValue="false" shared="true" includeChildSelections="true" includeChildForces="false" id="ce9d-8d2f-1811-d2de" type="max"/>
