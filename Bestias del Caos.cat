    <selectionEntry id="281a-c0f1-9287-e170" name="[E] - Morghur, Señor de las Calaveras" hidden="false" collective="false" import="true" type="unit">
      <modifiers>
        <modifier type="set" field="7b3a-99a1-0b4f-899f" value="0.0">
          <conditions>
            <condition field="selections" scope="roster" value="1.0" percentValue="false" shared="true" includeChildSelections="true" includeChildForces="false" childId="046a-1c36-705e-1c0e" type="equalTo"/>
