                <cost name="pts" typeId="points" value="30.0"/>
              </costs>
            </selectionEntry>
            <selectionEntry id="735f-b8ce-7afc-fa3d" name="Elegido de Hashut" hidden="false" collective="false" import="true" type="upgrade">
              <constraints>
                <constraint field="selections" scope="parent" value="1.0" percentValue="false" shared="false" includeChildSelections="false" includeChildForces="false" id="bf48-996e-e239-574f" type="max"/>
